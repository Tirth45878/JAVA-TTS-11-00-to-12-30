package CollectionDemo;

import java.util.HashMap;
import java.util.Map;

public class MapDemo {
	public static void main(String[] args) {
		Map<String, Double> map = new HashMap();
		map.put(1, "c");
		map.put("abc", 454.4);
		map.put("c++",2);
		map.put(false, "python");
		map.put(1, "python");
		System.out.println(map);
	}
}
