package constructor;
class Data1{
	int i;
	String name;
	long contact;
	Data1(){
		System.out.println("hello default constrctor");
	}
	Data1(int i){
		System.out.println("hello para constrctor");
	}
	Data1(int i,int j){
		System.out.println("hello para constrctor");
	}
	Data1(String i){
		System.out.println("hello para constrctor");
	}
	public void show() {
		System.out.println("i = "+i+" name = "+name+" contact = "+contact);
	}
}

public class Data {
	public static void main(String[] args) {
		Data1 d = new Data1(10);
		d.show();
	}
}
