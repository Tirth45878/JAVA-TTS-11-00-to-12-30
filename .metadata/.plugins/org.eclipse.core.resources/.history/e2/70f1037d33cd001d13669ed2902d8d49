package THreading;

public class ThreadDemo {
	public static void main(String[] args) {
		Thread t = Thread.currentThread();
		System.out.println(t);
		t.setName("MyThread");
		System.out.println(t);
		try {
			for(int i=1;i<=5;i++) {
				System.out.println(t+" : "+i);
				Thread.sleep(2000);
			}
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
